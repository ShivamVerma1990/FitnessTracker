            android:id="@+id/action_cardioFragment_to_optionsFragment"
            app:destination="@id/optionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/backFragment"
        android:name="com.candroid.realtracker.workout.BackFragment"
        android:label="fragment_back"
        tools:layout="@layout/fragment_back" >
