        <action
            android:id="@+id/action_optionsFragment_to_BFragment"
            app:destination="@id/BFragment" />
        <action
            android:id="@+id/action_optionsFragment_to_legFragment"
            app:destination="@id/legFragment" />
        <action
            android:id="@+id/action_optionsFragment_to_cardioFragment"
            app:destination="@id/cardioFragment" />
        <action
            android:id="@+id/action_optionsFragment_to_backFragment"
            app:destination="@id/backFragment" />
