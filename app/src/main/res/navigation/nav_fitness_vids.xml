        android:name="com.candroid.realtracker.workout.OptionsFragment"
        android:label="fragment_options"
        tools:layout="@layout/fragment_options" >
        <action
            android:id="@+id/action_optionsFragment_to_bicepsFragment"
            app:destination="@id/bicepsFragment" />
